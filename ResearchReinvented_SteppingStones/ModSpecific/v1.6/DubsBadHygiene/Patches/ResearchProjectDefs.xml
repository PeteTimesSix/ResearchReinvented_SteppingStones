<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<!-- Reposition due to space not taken up by removed RR:SS projects -->
	<Operation Class="RR.PatchOperationReplaceResearchCoords">
		<xpath>Defs/ResearchProjectDef[defName="RR_LateralThinking"]</xpath>
		<researchViewX>0.00</researchViewX>
		<researchViewY>5.50</researchViewY>
	</Operation>
	
	<Operation Class="RR.PatchOperationResearchPrereg">
		<xpath>Defs/ResearchProjectDef[defName="LogBoilers"]</xpath>
		<target>prerequisites</target>
		<defNames>
			<li>RR_Fire</li>
		</defNames>
	</Operation>
	<Operation Class="RR.PatchOperationResearchPrereg">
		<xpath>Defs/ResearchProjectDef[defName="SewageSludgeComposting"]</xpath>
		<target>prerequisites</target>
		<defNames>
			<li>RR_Agriculture</li>
		</defNames>
	</Operation>
	<Operation Class="RR.PatchOperationResearchPrereg">
		<xpath>Defs/ResearchProjectDef[defName="Plumbing"]</xpath>
		<target>prerequisites</target>
		<defNames>
			<li>Smithing</li>
		</defNames>
	</Operation>
</Patch>
